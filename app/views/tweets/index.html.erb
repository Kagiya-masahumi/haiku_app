<div class="contents row">
  <p>木村峻です！</p>
  <p>いいえ、渡辺綱之です！！</p>

  <% @tweets.each do |tweet| %>
    <div class="content_post" style="background-image: url(<%= tweet.image %>);">
      <p><%= tweet.text %></p>
      <span class="name">
        <%= tweet.name %>
      </span>
    </div>
  <% end %>
</div>